{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "FileApiOptions": {
    "UserImgDirectory": "img\\users",
    "GameImgDirectory": "img\\games",
    "UserFileTypes": [ ".jpeg", ".jpg", ".png" ],
    "GameFileTypes": [ ".jpeg", ".jpg", ".png" ],
    "DefaultUserImgPath":  "img\\default\\user-default.jpg",
    "DefaultGameImgPath":  "img\\default\\game-default.png"
  }
}
